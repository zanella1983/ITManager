@model ITManager.WebMvc.Models.ListUserViewModel

@{ 
    ViewData["Title"] = "Users";
}

<h2>@ViewData["Title"]</h2>

<table class="table table-striped table-hover">
    <thead>
        <tr class="success">
            <th>
                <p>ID</p>
            </th>
            <th>
                <p>Name</p>
            </th>
            <th>
                <p>Last Name</p>
            </th>
            <th>
                <p>Email</p>
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var dto in Model.Users)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => dto.Id)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => dto.Name)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => dto.LastName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => dto.Email)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => dto.Active)
                </td>
            </tr>
        }
    </tbody>
</table>

